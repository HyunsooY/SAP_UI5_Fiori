<mvc:View controllerName="zprojectodatae1402.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m" xmlns:t="sap.ui.table">
    <Page id="page" title="{i18n>title}">
        <content >
             <t:Table
				rows="{/CarrierSet}"
				selectionMode="MultiToggle"
				visibleRowCount="7"
				paste="onPaste"
				ariaLabelledBy="title">
				<t:extension>
					<OverflowToolbar style="Clear">
						<Title id="title" text="Products"/>
					</OverflowToolbar>
				</t:extension>
				<t:columns>
					<t:Column>
						<Label text="Carrid" />
						<t:template>
							<Text text="{Carrid}" wrapping="false" />
						</t:template>
					</t:Column>
                    <t:Column>
						<Label text="Connid" />
						<t:template>
							<Text text="{Connid}" wrapping="false" />
						</t:template>
					</t:Column>
                    <t:Column>
						<Label text="Cityfrom" />
						<t:template>
							<Text text="{Cityfrom}" wrapping="false" />
						</t:template>
					</t:Column>
                    <t:Column>
						<Label text="Cityto" />
						<t:template>
							<Text text="{Cityto}" wrapping="false" />
						</t:template>
					</t:Column>
				</t:columns>
			</t:Table>
        </content>
    </Page>
</mvc:View>